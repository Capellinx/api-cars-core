generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum Status {
  READ
  PENDING
}

model TrackLogs {
  id            String    @id @default(uuid()) @map("_id") @db.ObjectId
  carId         String    @map("car_id")
  dateCreated   DateTime  @map("date_created")
  dateProcessed DateTime? @map("date_processed")
  webhookStatus Status?   @default(PENDING) @map("webhook_status")

  @@map("track_logs")
}

model HistoryLogs {
  id      String @id @default(uuid()) @map("_id") @db.ObjectId
  message String

  @@map("history_logs")
}
